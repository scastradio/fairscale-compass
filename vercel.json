{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/login$", "dest": "/api/login.js" },
    { "src": "^/callback$", "dest": "/api/callback.js" },
    { "src": "^/fetch$", "dest": "/api/fetch.js" },
    { "src": "^/pfp$", "dest": "/api/pfp.js" },
    { "src": "^/logout$", "dest": "/api/logout.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
